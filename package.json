{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "test": "jest",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "dependencies": {
    "expo": "36.0.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-web": "0.11.7",
    "react-redux": "7.1.3",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@types/jest": "24.0.24",
    "@types/react": "16.9.0",
    "@types/react-native": "0.60.23",
    "@types/react-redux": "7.1.5",
    "babel-preset-expo": "8.0.0",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "ts-jest": "24.2.0",
    "tslint": "5.20.1",
    "tslint-functional-preset": "2.0.0",
    "typescript": "3.6.3"
  },
  "private": true,
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  }
}
